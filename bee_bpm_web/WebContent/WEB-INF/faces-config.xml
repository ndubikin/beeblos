  </resource-bundle>
  <resource-bundle>
   <base-name>org.beeblos.bpm.wc.taglib.util.messages</base-name>
   <var>msg</var>
  </resource-bundle>
  <message-bundle>org.beeblos.bpm.wc.taglib.util.jsf_messages</message-bundle>
  <variable-resolver>org.beeblos.bpm.wc.taglib.security.ContextoSeguridadVariableResolver</variable-resolver>
  <property-resolver>org.beeblos.bpm.wc.taglib.security.ContextoSeguridadPropertyResolver</property-resolver>
