 <navigation-rule>
  <display-name>Consulta Tarea</display-name>
  <from-view-id>/general/consultaTarea.xhtml</from-view-id>
  <navigation-case>
   <from-outcome>CONSULTA_TAREA</from-outcome>
   <to-view-id>/general/consultaTarea.xhtml</to-view-id>
  </navigation-case>
  <navigation-case>
   <from-outcome>FAIL</from-outcome>
   <to-view-id>/general/consultaTarea.xhtml</to-view-id>
  </navigation-case>
 </navigation-rule>
 <navigation-rule>
  <display-name>Login</display-name>
  <from-view-id>/login.xhtml</from-view-id>
  <navigation-case>
   <from-outcome>FAIL</from-outcome>
   <to-view-id>/login.xhtml</to-view-id>
  </navigation-case>
 </navigation-rule>
 <navigation-rule>
  <from-view-id>/login.xhtml</from-view-id>
  <navigation-case>
   <from-outcome>LOGIN</from-outcome>
   <to-view-id>/welcome.xhtml</to-view-id>
  </navigation-case>
  <navigation-case>
   <from-outcome>LOGOUT</from-outcome>
   <to-view-id>/login.xhtml</to-view-id>
  </navigation-case>
 </navigation-rule>
 <navigation-rule>
  <from-view-id>*</from-view-id>
  <navigation-case>
   <from-outcome>PROCESS_TASK</from-outcome>
   <to-view-id>/task/processWithForm.xhtml</to-view-id>
  </navigation-case>
  <navigation-case>
   <from-outcome>TASK_PROCESSED_OK</from-outcome>
   <to-view-id>/task/processWellcomePage.xhtml</to-view-id>
   <redirect/>
  </navigation-case>
  <navigation-case>
   <from-outcome>PROCESS_WELLCOME_PAGE</from-outcome>
   <to-view-id>/task/processWellcomePage.xhtml</to-view-id>
   <redirect/>
  </navigation-case>
  <navigation-case>
   <from-outcome>WELCOME_PAGE</from-outcome>
   <to-view-id>/welcome.xhtml</to-view-id>
   <redirect/>
  </navigation-case>
  <navigation-case>
   <from-outcome>LOGOUT</from-outcome>
   <to-view-id>/login.xhtml</to-view-id>
   <redirect/>
  </navigation-case>
  <navigation-case>
   <from-outcome>GO_TO_LOGIN</from-outcome>
   <to-view-id>/login.xhtml</to-view-id>
   <redirect/>
  </navigation-case>
  <navigation-case>
   <from-outcome>SAVE_CONF_FAIL</from-outcome>
   <to-view-id>/login.xhtml</to-view-id>
   <redirect/>
  </navigation-case>
  <navigation-case>
   <from-outcome>CONSULTA_USUARIO_LOGIN</from-outcome>
   <to-view-id>/general/consultaUsuarioLogin.xhtml</to-view-id>
   <redirect/>
  </navigation-case>
  <navigation-case>
   <from-outcome>SUCCESS_USUARIO</from-outcome>
   <to-view-id>/mantenimientos/usuarioCrud.xhtml</to-view-id>
  </navigation-case>
  <navigation-case>
   <from-outcome>WPROCESSDEF_QUERY</from-outcome>
   <to-view-id>/def/wProcessDefCrud.xhtml</to-view-id>
  </navigation-case>
  <navigation-case>
   <from-outcome>WORKINGPROCESS_QUERY</from-outcome>
   <to-view-id>/admin/workingProcessQueryCall.xhtml</to-view-id>
  </navigation-case>
  <navigation-case>
   <from-outcome>WORKINGWORKS_QUERY</from-outcome>
   <to-view-id>/admin/workingWorkQueryCall.xhtml</to-view-id>
  </navigation-case>
  <navigation-case>
   <from-outcome>WORKINGSTEPS_QUERY</from-outcome>
   <to-view-id>/admin/workingStepQueryCall.xhtml</to-view-id>
  </navigation-case>
  <navigation-case>
   <from-outcome>LOAD_WPROCESSWORK</from-outcome>
   <to-view-id>/forms/wProcessWorkFormCall.xhtml</to-view-id>
  </navigation-case>
  <navigation-case>
   <from-outcome>LOAD_WPROCESSDEF</from-outcome>
   <to-view-id>/forms/wProcessDefFormCall.xhtml</to-view-id>
  </navigation-case>
  <navigation-case>
   <from-outcome>LOAD_WPROCESSHEAD</from-outcome>
   <to-view-id>/forms/wProcessHeadFormCall.xhtml</to-view-id>
  </navigation-case>
  <navigation-case>
   <from-outcome>CREATE_NEW_WPROCESSDEF</from-outcome>
   <to-view-id>/forms/wProcessDefFormCall.xhtml</to-view-id>
  </navigation-case>
  <navigation-case>
   <from-outcome>CREATE_NEW_WPROCESS</from-outcome>
   <to-view-id>/forms/wProcessHeadFormCall.xhtml</to-view-id>
  </navigation-case>
  <navigation-case>
   <from-outcome>WSTEPDEF_QUERY</from-outcome>
   <to-view-id>/def/wStepDefCrud.xhtml</to-view-id>
  </navigation-case>
  <navigation-case>
   <from-outcome>LOAD_WSTEPDEF</from-outcome>
   <to-view-id>/forms/wStepDefFormCall.xhtml</to-view-id>
  </navigation-case>
  <navigation-case>
   <from-outcome>LOAD_WSTEPHEAD</from-outcome>
   <to-view-id>/forms/wStepHeadFormCall.xhtml</to-view-id>
  </navigation-case>
  <navigation-case>
   <from-outcome>CREATE_NEW_WSTEPDEF</from-outcome>
   <to-view-id>/forms/wStepDefFormCall.xhtml</to-view-id>
  </navigation-case>
  <navigation-case>
   <from-outcome>CREATE_NEW_WSTEPHEAD</from-outcome>
   <to-view-id>/forms/wStepHeadFormCall.xhtml</to-view-id>
  </navigation-case>
 </navigation-rule>
 <navigation-rule>
  <from-view-id>/welcome.xhtml</from-view-id>
  <navigation-case>
   <from-outcome>SUCCESS_WELCOME</from-outcome>
   <to-view-id>/welcome.xhtml</to-view-id>
  </navigation-case>
  <navigation-case>
   <from-outcome>FAIL</from-outcome>
   <to-view-id>/welcome.xhtml</to-view-id>
  </navigation-case>
 </navigation-rule>
 <navigation-rule>
  <display-name>WProcessDefForm</display-name>
  <from-view-id>/forms/wProcessDefFormCall.xhtml</from-view-id>
  <navigation-case>
   <from-outcome>FAIL</from-outcome>
   <to-view-id>/forms/wProcessDefFormCall.xhtml</to-view-id>
  </navigation-case>
  <navigation-case>
   <from-outcome>SUCCESS_FORM_WPROCESSDEF</from-outcome>
   <to-view-id>/forms/wProcessDefFormCall.xhtml</to-view-id>
  </navigation-case>
  <navigation-case>
   <from-outcome>SUCCESS_FORM_WPROCESS</from-outcome>
   <to-view-id>/forms/wProcessHeadFormCall.xhtml</to-view-id>
  </navigation-case>
  <!-- dml 20120305 -->
  <navigation-case>
   <from-outcome>WPROCESSDEF_QUERY</from-outcome>
   <to-view-id>/def/wProcessDefCrud.xhtml</to-view-id>
  </navigation-case>
 </navigation-rule>
 <navigation-rule>
  <display-name>WProcessForm</display-name>
  <from-view-id>/forms/wProcessHeadFormCall.xhtml</from-view-id>
  <navigation-case>
   <from-outcome>FAIL</from-outcome>
   <to-view-id>/forms/wProcessHeadFormCall.xhtml</to-view-id>
  </navigation-case>
  <navigation-case>
   <from-outcome>SUCCESS_FORM_WPROCESSDEF</from-outcome>
   <to-view-id>/forms/wProcessDefFormCall.xhtml</to-view-id>
  </navigation-case>
 </navigation-rule>
 <navigation-rule>
  <display-name>WStepHeadForm</display-name>
  <from-view-id>/forms/wStepHeadFormCall.xhtml</from-view-id>
  <navigation-case>
   <from-outcome>FAIL</from-outcome>
   <to-view-id>/forms/wStepHeadFormCall.xhtml</to-view-id>
  </navigation-case>
  <navigation-case>
   <from-outcome>SUCCESS_FORM_WSTEPDEF</from-outcome>
   <to-view-id>/forms/wStepDefFormCall.xhtml</to-view-id>
  </navigation-case>
 </navigation-rule>
 <navigation-rule>
  <display-name>WStepDefForm</display-name>
  <from-view-id>/forms/wStepDefFormCall.xhtml</from-view-id>
  <navigation-case>
   <from-outcome>FAIL</from-outcome>
   <to-view-id>/forms/wStepDefFormCall.xhtml</to-view-id>
  </navigation-case>
  <navigation-case>
   <from-outcome>SUCCESS_FORM_WSTEPDEF</from-outcome>
   <to-view-id>/forms/wStepDefFormCall.xhtml</to-view-id>
  </navigation-case>
  <navigation-case>
   <from-outcome>SUCCESS_FORM_WSTEPHEAD</from-outcome>
   <to-view-id>/forms/wStepHeadFormCall.xhtml</to-view-id>
  </navigation-case>
  <!-- dml 20120305 -->
  <navigation-case>
   <from-outcome>WSTEPDEF_QUERY</from-outcome>
   <to-view-id>/def/wStepDefCrud.xhtml</to-view-id>
  </navigation-case>
 </navigation-rule>
 <application>
  <resource-bundle>
   <base-name>org.beeblos.bpm.wc.taglib.util.error_messages</base-name>
