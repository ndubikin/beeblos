<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.email.tray.core.model.EmailTray" table="email_tray">
		<id name="id" type="java.lang.Integer">
			<column name="id" />
			<generator class="identity" />
		</id>
		<property name="to" type="string">
			<column name="to_account" />
		</property>
		<property name="from" type="string">
			<column name="from_account" />
		</property>
		<property name="cc" type="string">
			<column name="cc" />
		</property>
		<property name="bcc" type="string">
			<column name="bcc" />
		</property>
		<property name="subject" type="string">
			<column name="subject" />
		</property>
		<property name="originalSubject" type="string">
			<column name="original_subject" />
		</property>
		<property name="messageId" type="string">
			<column name="message_id" />
		</property>
		<property name="encode" type="string">
			<column name="encode" />
		</property>
        <property name="body" >
            <column name="body" sql-type="LONGTEXT"/>
        </property>
		<property name="inputDate"
			type="org.jadira.usertype.dateandtime.joda.PersistentDateTime">
			<column name="input_date" />
		</property>
		<property name="objectId" type="java.lang.Integer">
			<column name="object_id" />
		</property>
		<property name="objectClass" type="string">
			<column name="object_class" />
		</property>
		<property name="readDate"
			type="org.jadira.usertype.dateandtime.joda.PersistentDateTime">
			<column name="read_date" />
		</property>
		<property name="repliedEmailTrayId" type="java.lang.Integer">
			<column name="replied_email_tray_id" />
		</property>
        <property name="repliedDate"
			type="org.jadira.usertype.dateandtime.joda.PersistentDateTime">
            <column name="replied_Date"/>
        </property>
		<property name="forwardedEmailTrayId" type="java.lang.Integer">
			<column name="forwarded_email_tray_id" />
		</property>
         <property name="forwardedDate"
			type="org.jadira.usertype.dateandtime.joda.PersistentDateTime">
            <column name="forwarded_date"/>
        </property>
		<property name="type" type="string">
			<column name="type" />
		</property>
		<property name="deleted" type="java.lang.Boolean">
			<column name="deleted" sql-type="bit" />
		</property>

		<property name="addDate" type="org.jadira.usertype.dateandtime.joda.PersistentDateTime">
			<column name="add_date" />
		</property>
		<property name="addUser" type="java.lang.Integer">
			<column name="add_user" />
		</property>
		<property name="modDate" type="org.jadira.usertype.dateandtime.joda.PersistentDateTime">
			<column name="mod_date" />
		</property>
		<property name="modUser" type="java.lang.Integer">
			<column name="mod_user" />
		</property>

		<many-to-one class="com.email.tray.core.model.EmailTag"
			column="email_tag_id" lazy="false" fetch="join" name="tag" />

 		<many-to-one class="com.sp.common.core.model.noper.UserThin" 
 			column="read_user_id" lazy="false" fetch="join" name="readUser" /> 

 		<many-to-one class="com.sp.common.core.model.noper.UserThin" 
 			column="user_id" lazy="false" fetch="join" name="user" /> 

 		<many-to-one class="com.sp.common.core.model.noper.UserThin" 
 			column="replied_user_id" lazy="false" fetch="join" name="repliedUser" /> 

 		<many-to-one class="com.sp.common.core.model.noper.UserThin" 
 			column="forwarded_user_id" lazy="false" fetch="join" name="forwardedUser" /> 

	</class>
	
	<class name="com.email.tray.core.model.EmailTrayAttachment" table="email_tray_attachment">
		<id name="id" type="java.lang.Integer">
			<column name="id" />
			<generator class="identity" />
		</id>
		
		<property name="name" type="string">
			<column name="name" />
		</property>
		<property name="mime" type="string">
			<column name="mime" />
		</property>
		<property name="attachmentLength" type="java.lang.Integer">
			<column name="attachment_length" />
		</property>
		<property name="attachment" >
			<column name="attachment" sql-type="LONGBLOB"/>
		</property>

		<property name="addDate" type="org.jadira.usertype.dateandtime.joda.PersistentDateTime">
			<column name="add_date" />
		</property>
		<property name="addUser" type="java.lang.Integer">
			<column name="add_user" />
		</property>
		<property name="modDate" type="org.jadira.usertype.dateandtime.joda.PersistentDateTime">
			<column name="mod_date" />
		</property>
		<property name="modUser" type="java.lang.Integer">
			<column name="mod_user" />
		</property>

		<many-to-one class="com.email.tray.core.model.EmailTray"
			column="email_tray_id" lazy="false" fetch="join" name="emailTray" />

	</class>
	
	<class name="com.email.tray.core.model.EmailTag" table="email_tag">
		<id name="id" type="java.lang.Integer">
			<column name="id" />
			<generator class="identity" />
		</id>
		<property name="name" type="string">
			<column name="name" />
		</property>
		<property name="colorTag" type="string">
			<column name="color_tag" />
		</property>
		<property name="description" type="string">
			<column name="description" />
		</property>

	</class>

	<class name="com.sp.daemon.email.EmailDConf" table="email_d_conf">
		<id name="id" type="java.lang.Integer">
			<column name="id" />
			<generator class="identity" />
		</id>
		<property name="enabled" type="java.lang.Boolean">
			<column name="enabled" sql-type="bit"/>
		</property>
		<property name="pollingFrequency" type="java.lang.Integer">
			<column name="polling_frequency" />
		</property>
		<property name="daemonClassImplementationName" type="java.lang.String">
			<column name="daemon_class_implementation_name" />
		</property>
		<property name="docClassId" type="java.lang.Integer">
			<column name="doc_class_id" />
		</property>
		<property name="docClassName" type="string">
			<column name="doc_class_name" />
		</property>
		<property name="beeblosServerId" type="java.lang.Integer">
			<column name="beeblos_server_id" />
		</property>
		<property name="inputFolder" type="string">
			<column name="input_folder" />
		</property>
		<property name="markAsRead" type="java.lang.Boolean">
			<column name="mark_as_read" sql-type="bit"/>
		</property>
		<property name="leaveOnInputFolder" type="java.lang.Boolean">
			<column name="leave_on_input_folder" sql-type="bit"/>
		</property>
		<property name="validEmailFolder" type="string">
			<column name="valid_email_folder" />
		</property>
		<property name="moveToValidFolder" type="java.lang.Boolean">
			<column name="move_to_valid_folder" sql-type="bit"/>
		</property>
		<property name="invalidEmailFolder" type="string">
			<column name="invalid_email_folder" />
		</property>
		<property name="moveToInvalidFolder" type="java.lang.Boolean">
			<column name="move_to_invalid_folder" sql-type="bit"/>
		</property>
		<property name="errorEmailFolder" type="string">
			<column name="error_email_folder" />
		</property>
		<property name="moveToErrorFolder" type="java.lang.Boolean">
			<column name="move_to_error_folder" sql-type="bit"/>
		</property>
		<property name="checkingFieldName" type="string">
			<column name="checking_field_name" />
		</property>
		<property name="checkingFieldValue" type="string">
			<column name="checking_field_value" />
		</property>
		<property name="type" type="string">
			<column name="type" />
		</property>

		<property name="addDate" type="org.jadira.usertype.dateandtime.joda.PersistentDateTime">
			<column name="add_date" />
		</property>
		<property name="addUser" type="java.lang.Integer">
			<column name="add_user" />
		</property>
		<property name="modDate" type="org.jadira.usertype.dateandtime.joda.PersistentDateTime">
			<column name="mod_date" />
		</property>
		<property name="modUser" type="java.lang.Integer">
			<column name="mod_user" />
		</property>

		<many-to-one class="com.sp.common.core.model.UserEmailAccount"
			column="user_email_account_id" lazy="false" fetch="join" name="emailAccount" />
			
		<many-to-one class="com.sp.common.core.model.EmailTemplate"
			column="email_template_id" lazy="false" fetch="join" name="emailTemplate" />

	</class>

	<class name="com.sp.daemon.email.EmailDLog" table="email_d_log">
		<id name="id" type="java.lang.Integer">
			<column name="id" />
			<generator class="identity" />
		</id>
		<property name="daemonConfId" type="java.lang.Integer">
			<column name="email_d_conf_id" />
		</property>
		<property name="processed" type="java.lang.Boolean">
			<column name="processed" sql-type="bit" />
		</property>
		<property name="processedDate"
			type="org.jadira.usertype.dateandtime.joda.PersistentDateTime">
			<column name="processed_date" />
		</property>
		<property name="userId" type="java.lang.Integer">
			<column name="user_id" />
		</property>
		<property name="objectId" type="java.lang.Integer">
			<column name="object_id" />
		</property>
		<property name="objectClass" type="string">
			<column name="object_class" />
		</property>
		<property name="from" type="java.lang.String">
			<column name="from_account" />
		</property>
		<property name="messageId" type="java.lang.String">
			<column name="message_id" />
		</property>
		<property name="storeFolder" type="java.lang.String">
			<column name="store_folder" />
		</property>

	</class>

	<class name="com.sp.daemon.email.EmailDPoll" table="email_d_poll">
		<id name="id" type="java.lang.Integer">
			<column name="id" />
			<generator class="identity" />
		</id>
		<property name="daemonConfId" type="java.lang.Integer">
			<column name="email_d_conf_id" />
		</property>
		<property name="beginDate"
			type="org.jadira.usertype.dateandtime.joda.PersistentDateTime">
			<column name="begin_date" />
		</property>
		<property name="endDate"
			type="org.jadira.usertype.dateandtime.joda.PersistentDateTime">
			<column name="end_date" />
		</property>
		<property name="threadName" type="string">
			<column name="thread_name" />
		</property>
		<property name="qtyInputEmail" type="java.lang.Integer">
			<column name="qty_input_email" />
		</property>
		<property name="qtyValidEmail" type="java.lang.Integer">
			<column name="qty_valid_email" />
		</property>
		<property name="qtyInvalidEmail" type="java.lang.Integer">
			<column name="qty_invalid_email" />
		</property>
		<property name="qtyErrorEmail" type="java.lang.Integer">
			<column name="qty_error_email" />
		</property>
		<property name="comment" type="string">
			<column name="comment" />
		</property>
		<property name="commentLastModified" type="org.jadira.usertype.dateandtime.joda.PersistentDateTime">
			<column name="comment_last_modified" />
		</property>

		<property name="addDate" type="org.jadira.usertype.dateandtime.joda.PersistentDateTime">
			<column name="add_date" />
		</property>
		<property name="addUser" type="java.lang.Integer">
			<column name="add_user" />
		</property>
		<property name="modDate" type="org.jadira.usertype.dateandtime.joda.PersistentDateTime">
			<column name="mod_date" />
		</property>
		<property name="modUser" type="java.lang.Integer">
			<column name="mod_user" />
		</property>

	</class>

</hibernate-mapping>
